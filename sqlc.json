{
  "version": "1",
  "overrides": [
    {
      "column": "devices.data",
      "go_type": "database/sql.NullString"
    },
    {
      "column": "devices.meta",
      "go_type": "database/sql.NullString"
    }
  ],
  "packages": [
    {
      "path": "pkg/common/repository",
      "name": "repository",
      "schema": "db/migrations",
      "queries": "db/queries",
      "engine": "postgresql",
      "emit_json_tags": true,
      "emit_prepared_queries": true,
      "emit_interface": true
    }
  ]
}